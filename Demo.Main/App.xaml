            <Style x:Key="WindowsCloseButton" TargetType="{x:Type Button}">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Grid Width="{TemplateBinding Width}"
                                  Height="{TemplateBinding Height}"
                                  HorizontalAlignment="Left"
                                  VerticalAlignment="Top">
                                <VisualStateManager.VisualStateGroups>
                                    <VisualStateGroup x:Name="CommonStates">
                                        <VisualState x:Name="Normal" />
                                        <VisualState x:Name="MouseOver">
                                            <Storyboard>
                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Inner" Storyboard.TargetProperty="(UIElement.Opacity)">
                                                    <EasingDoubleKeyFrame KeyTime="0" Value="0.995" />
                                                </DoubleAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </VisualState>
                                        <VisualState x:Name="Pressed" />
                                        <VisualState x:Name="Disabled" />
                                    </VisualStateGroup>
                                    <VisualStateGroup x:Name="FocusStates">
                                        <VisualState x:Name="Unfocused" />
                                        <VisualState x:Name="Focused">
                                            <Storyboard>
                                                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="Inner" Storyboard.TargetProperty="(UIElement.Opacity)">
                                                    <EasingDoubleKeyFrame KeyTime="0" Value="0.8" />
                                                </DoubleAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </VisualState>
                                    </VisualStateGroup>
                                </VisualStateManager.VisualStateGroups>
                                <controls:DropdownShadow x:Name="dropdownShadow"
                                                         BottomMiddleBrush="{DynamicResource LightGlassRed__bm}"
                                                         BottomOffset="10"
                                                         BottomRightBrush="{DynamicResource LightGlassRed__br}"
                                                         LeftBottomBrush="{DynamicResource LightGlassRed__bl}"
                                                         LeftMiddleBrush="{DynamicResource LightGlassRed__lm}"
                                                         LeftOffset="10"
                                                         RightMiddleBrush="{DynamicResource LightGlassRed__rm}"
                                                         RightOffset="10"
                                                         RoundMargin="-5"
                                                         TopLeftBrush="{DynamicResource LightGlassRed__tl}"
                                                         TopMiddleBrush="{DynamicResource LightGlassRed__tm}"
                                                         TopOffset="10"
                                                         TopRightBrush="{DynamicResource LightGlassRed__tr}"
                                                         Visibility="Collapsed" />
                                <Border x:Name="Outer"
                                        BorderBrush="{TemplateBinding BorderBrush}"
                                        BorderThickness="{TemplateBinding BorderThickness}"
                                        CornerRadius="0,0,3,0" />
                                <Border x:Name="Inner"
                                        Background="{TemplateBinding Background}"
                                        BorderBrush="White"
                                        BorderThickness="0.5"
                                        CornerRadius="0,0,3,0">
                                    <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                      VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                                      RecognizesAccessKey="True"
                                                      SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                                </Border>
                            </Grid>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsFocused" Value="True" />
                                <Trigger Property="IsDefaulted" Value="True" />
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter TargetName="dropdownShadow" Property="Visibility" Value="Visible" />
                                </Trigger>
                                <Trigger Property="IsPressed" Value="True" />
                                <Trigger Property="IsEnabled" Value="False" />
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                <Setter Property="Background" Value="{DynamicResource DarkGlassRedBrush}" />
                <Setter Property="BorderBrush" Value="{DynamicResource GrayBorderBrush}" />
                <Setter Property="BorderThickness" Value="0,0,1,1" />
                <Style.Triggers>
                    <Trigger Property="IsFocused" Value="True">
                        <Setter Property="Background" Value="{DynamicResource lightGlassRed}" />
                        <Setter Property="BorderBrush" Value="#FFD4D4E0" />
                    </Trigger>
                    <Trigger Property="IsMouseOver" Value="False">
                        <Setter Property="Background" Value="{DynamicResource DarkGlassRedBrush}" />
                    </Trigger>
                    <Trigger Property="IsMouseOver" Value="True">
                        <Setter Property="Background" Value="{DynamicResource lightGlassRed}" />
                    </Trigger>
                </Style.Triggers>
            </Style>
